{
  "name": "challenges-frontend-mentor",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "dev:qr": "turbo run dev --filter=./apps/qr-component",
    "dev:chart": "turbo run dev --filter=./apps/chart-component",
    "dev:product": "turbo run dev --filter=./apps/product-preview",
    "dev:grid": "turbo run dev --filter=./apps/testimonial-grid",
    "dev:crowd": "turbo run dev --filter=./apps/crowdfunding-product-page",
    "dev:card": "turbo run dev --filter=./apps/interactive-card-details-form",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.19"
}
